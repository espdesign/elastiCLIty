{
  "linux": {
    "module 0": {
      "Display the date and time.": "date",
      "Display the month as a calender.": "cal",
      "Display the name of current user.": "whoami",
      "Display the name of currently logged-in computer.": "hostname",
      "Identify the name of operating system kernel": "uname",
      "Display the CPU information": "lscpu",
      "Identify the RAM information": "free",
      "List the storage devices attached to the system": "lsblk",
      "List the free storage information": "df",
      "Identify the system's network settings": "ifconfig",
      "Identify the other logged-in users": "who"
    },
    "module 1": {
      "Display the date and time one week from now.": "date -d \"week\"",
      "Display the calender month in Nov 92": "cal nov 92",
      "Identify the details of the operating system": "uname -a"
    }
  },
